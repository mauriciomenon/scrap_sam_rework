# SCRAP_SAM - Variáveis de Ambiente
# Copie este arquivo para .env e configure os valores apropriados

# Configurações do Playwright
PLAYWRIGHT_BROWSERS_PATH=./drivers
PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=0

# Configurações do Selenium
SELENIUM_WEBDRIVER_PATH=./drivers
SELENIUM_HEADLESS=1

# Configurações de Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/scrap_sam.log

# Configurações do Dashboard
DASH_HOST=0.0.0.0
DASH_PORT=8050
DASH_DEBUG=False

# Configurações de Timeout
REQUEST_TIMEOUT=30
PAGE_LOAD_TIMEOUT=60

# Configurações de Banco de Dados (se aplicável)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=scrap_sam
DB_USER=user
DB_PASSWORD=password

# Configurações de API (se aplicável)
API_BASE_URL=https://api.example.com
API_KEY=your_api_key_here

# Configurações de Segurança
SECRET_KEY=your_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
